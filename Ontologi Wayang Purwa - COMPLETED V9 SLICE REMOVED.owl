        <wayangpurwa1:kesaktianDari rdf:resource="http://ukdw.ac.id/ontologi/wayangpurwa#prabu_dwarawati"/>
        <rdfs:comment>Kesaktian Sri Kresna dan Harjuna Sasrabahu yang bisa berubah wujud menjadi raksasa kosmik dan menunjukan kekuatan sejati dewa.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://ukdw.ac.id/ontologi/wayangpurwa#tombak_karawelang -->

    <owl:NamedIndividual rdf:about="http://ukdw.ac.id/ontologi/wayangpurwa#tombak_karawelang">
