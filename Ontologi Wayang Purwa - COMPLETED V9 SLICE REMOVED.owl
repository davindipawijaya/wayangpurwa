    <!-- http://ukdw.ac.id/ontologi/wayangpurwa#aji_pecagragam -->

    <owl:NamedIndividual rdf:about="http://ukdw.ac.id/ontologi/wayangpurwa#aji_pecagragam">
        <rdf:type rdf:resource="http://ontology/wayangpurwa#Kesaktian"/>
